 <form class="col-md-6 col-md-offset-3 cust-form-add">
  <div class="form-group">
    <input id="comm-text" type="text" class="form-control input-lg" placeholder="Comment Text" />
  </div>
   <div class="form-group">
        <button id="sumbit-comment" class="btn btn-primary btn-success">Submit comment</button>
 </form>
  </div>
  <form class="col-md-2 col-md-offset-3 cust-form-search">
  <div class="form-group">
    <input id="search-text" type="text" class="form-control input-sm" placeholder="Search Pattern" />
  </div>
   <div class="form-group">
        <button id="sumbit-search" class="btn btn-primary btn-info">Search</button>
 </form>
  </div> 
{{#this.result}}
<div class="row">
<div id="{{id}}" class="container col-md-6 col-md-offset-2">  
  <h2><a href="http://localhost:3001/?#/materials/{{id}}">{{title}}</a></h2>
  <img src="{{img}}" class="img-rounded" alt="Cinque Terre" width="304" height="170">
  {{#if description}}
    <h4>Description: {{description}}</h4>
  {{/if}}
  <p><span>Rating: {{rating}} </span>  <span>: Comments: {{commentsCount}}</span></p>
  <p>Created on {{createdOn}} by <a href="http://localhost:3001/?#/profiles/{{user.username}}">{{user.username}}</a></p>
  <button  class="btn btn-primary maybe-hidden add-comment">Comment</button>
</div>
{{/this.result}}
